{
    "name": "Monhack",
    "description": "MONster Documenttion for HACKers",
    "version": "1.0.1",
    "manifest_version": 2,
    "background": {
        "scripts": ["popup.js"],
        "persistent": false
    },
    "popup": {
        "default_title": "Monhack",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "commands": {
        "_execute_popup": {
            "suggested_key": {
                "default": "Alt+M",
                "mac": "Alt+M"
            }
        }
    }
}
