{
    "name": "Monhack",
    "version": "1.0",
    "description": "Monster Documentation for Hacker",
    "permissions": [
        "bookmarks"
    ],
    "commands": {
        "toggle-feature-foo": {
            "suggested_key": {
                "default": "Ctrl+Shift+M",
                "mac": "Command+Shift+M"
            },
            "description": "Toggle feature foo"
        }
    },
    "browser_action": {
    "default_title": "Monhack",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
